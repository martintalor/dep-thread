    <select id="getNeedEtlInfoRecords" resultMap="BaseResultMap">
        select DISTINCT t1.PACKAGE_ID
          from section_step_recorders t1
          where not exists ( select 1 from section_step_recorders t2
            where t1.package_id = t2.package_id
            and t2.section_name='class com.iflytek.dep.server.section.InToDatabaseSection'
            and t2.doact_result = 1
          )
           and t1.section_name='class com.iflytek.dep.server.section.InToDatabaseSection'
    </select>


